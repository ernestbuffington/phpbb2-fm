<table align="center" cellpadding="4" cellspacing="1" width="100%" class="forumline">
<tr> 
	<th colspan="3" class="thHead">{L_PRIVATE_MESSAGES}</th>
</tr>
<tr> 
	<td class="row2" nowrap="nowrap"><b class="genmed">{L_FROM}:</b></td>
	<td width="100%" class="row2"><span class="genmed">{FROM}{FROM_IP}</span></td>
</tr>
<tr> 
	<td class="row2" nowrap="nowrap"><b class="genmed">{L_TO}:</b></td>
	<td class="row2"><span class="genmed">{TO}</span></td>
</tr>
<tr> 
	<td class="row2" nowrap="nowrap"><b class="genmed">{L_SENT_DATE}:</b></td>
	<td class="row2"><span class="genmed">{DATE}</span></td>
</tr>
<tr> 
	<td class="row2" nowrap="nowrap"><b class="genmed">{L_SUBJECT}:</b></td>
	<td class="row2"><span class="genmed">{SUBJECT}</span></td>
</tr>
<tr> 
	  <td valign="top" colspan="2" class="row1"><span class="postbody">{MESSAGE}</span></td>
</tr>
</table><br />
