<!-- BEGIN daily_game -->
<table cellpadding="4" cellspacing="1" class="forumline" align="center" width="100%">
<tr>
	<th class="thTop" colspan="4">{daily_game.TITLE}</th>
</tr>	
<tr>
	<td class="{daily_game.ROW_CLASS}" width="20%">
	<fieldset class="fieldset" style="margin: 0px 0px 0px 0px;">
	<legend><b>{daily_game.PROPER_NAME}</b></legend>
	<table align="center" width="100%" valign="top">
	<tr>
		<td width="50%">{daily_game.NEW_I_LINK}{daily_game.IMAGE_LINK}</a></td>
		<td align="center" width="50%">{daily_game.KEYBOARD}{daily_game.MOUSE}</td>		
	</tr>
	<tr>
		<td colspan="2"><span class="genmed">{daily_game.LINKS}{daily_game.DOWNLOAD_LINK}</span></td>		
	</tr>	
	</table>								
	</fieldset>
	</td>
	<td class="{daily_game.ROW_CLASS}" width="15%"><span class="genmed">{daily_game.TROPHY_IMG} {daily_game.TOP_PLAYER}<br />{daily_game.TOP_SCORE}<br /><br />{daily_game.RUNNER_IMG} {daily_game.BEST_PLAYER}<br />{daily_game.BEST_SCORE}<br /><br />{daily_game.FAVORITE_GAME}</span></td>
	<td class="{daily_game.ROW_CLASS}" width="20%"><span class="genmed">{daily_game.SEPERATOR}<a href="{daily_game.COMMENTS}">{daily_game.L_COMMENTS}</a>{daily_game.CHALLENGE}{daily_game.LIST}<br />{daily_game.SEPERATOR}<a href="{daily_game.STATS}">{daily_game.INFO}</a><br />{daily_game.GAMES_PLAYED} {daily_game.I_PLAYED}<br /><center>{daily_game.POP_PIC}</center></span></td>				
	<td class="{daily_game.ROW_CLASS}" width="45%">
	<fieldset class="fieldset" style="margin: 0px 0px 0px 0px;">
	<legend>{daily_game.DESC2}</legend>
	{daily_game.DESC}
	</fieldset>
	<br />
	<fieldset class="fieldset" style="margin: 0px 0px 0px 0px;">
	<legend>{daily_game.RATING_TITLE}</legend>
	<br />{daily_game.SEPERATOR}{daily_game.RATING_SENT} {daily_game.RATING_SUBMIT} {daily_game.RATING_IMAGE}<br />
	</fieldset>				
	</td>
</tr>
</table>
<br />
<!-- END daily_game -->
